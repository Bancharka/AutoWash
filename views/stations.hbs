<div class="flex flex-col gap-xl"> 
    <h1>{{title}}</h1>
        <div class="flex flex-col gap-lg">
            <div class="flex flex-col gap-md">
                {{>button href="/add-station" type="button" text="Tilføj station" variant="secondary"}}
                {{>button href="/add-company" type="button" text="Tilføj firma" variant="secondary"}}
            </div>
            <div class="flex flex-col gap-sm">
                {{#each stations}}
                    {{>itemCard href=(concat "/stations/" id) title=companies.name body=(array address (concat postalCode ", " city))}}
                {{/each}}
            </div>
        </div>
    </div>
    {{>footerNav}}
</div>